CREATE TABLE TRACKER_REQUISITION_PRINT
(
	print_requisition_id serial NOT NULL,
	issue_min_date date NOT NULL,
	issue_max_date date NOT NULL,
	transaction_id varchar(100) NOT NULL,
	delivery_min_date date NOT NULL,
	delivery_max_date date NOT NULL,
	category_name text NOT NULL,
	style_code varchar(50) NOT NULL,
	designer_code varchar(20) NOT NULL,
	color_code varchar(10) NOT NULL,
	product_codes varchar(500) NOT NULL,
	item_code varchar(50) NOT NULL,
	production_types varchar(200) NOT NULL,
	PRINT_code varchar(50) NOT NULL,
	printing_type varchar(20) NOT NULL,
	print_colour varchar(20) NOT NULL,
	num_screens_blocks int NOT NULL,
	num_colours int NOT NULL,
	print_length float NOT NULL,
	print_width float NOT NULL,
	print_pcs int NOT NULL,
	print_total_measurement float NOT NULL,
	sample_reference varchar(20) NOT NULL,
	sc_planning_main_item_id int NOT NULL,
	style_ccode_id int NOT NULL,
	sc_asis_plan_print_item_id int NOT NULL,
	cat_custom_plan_print_item_id int NOT NULL,
	sc_custom_dept_plan_print_item_id int NOT NULL,
	custom char(1) NOT NULL,
	dept_changed char(1) NOT NULL,
	approved char(1) NOT NULL,
	PRIMARY KEY (print_requisition_id)
) WITHOUT OIDS;
