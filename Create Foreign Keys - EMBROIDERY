/* Create Foreign Keys */

ALTER TABLE TRACKER_CUSTOM_ORDERS_EMBROIDERY
	ADD FOREIGN KEY (order_id)
	REFERENCES TRACKER_ORDERS_EMBROIDERY (order_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;


ALTER TABLE TRACKER_ORDERS_EMBROIDERY
	ADD FOREIGN KEY (embrd_requisition_id)
	REFERENCES TRACKER_REQUISITION_EMBROIDERY (embrd_requisition_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;
