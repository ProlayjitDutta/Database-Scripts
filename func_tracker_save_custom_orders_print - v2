CREATE OR REPLACE FUNCTION func_tracker_save_custom_orders_print(
		IN v_order_id int,
		IN v_print_length float,
		IN v_print_width float,
		IN v_print_pcs int,
		IN v_print_total_measurement float,
    OUT output_msg text,
    OUT id integer)
  RETURNS record AS
$BODY$
BEGIN   
					
		INSERT INTO tracker_custom_received_orders_print (
		order_id,
		print_length,
		print_width,
		print_pcs,
		print_total_measurement,
		received_date
		)
		VALUES(			
		$1,
		$2,
		$3,
		$4,
		$5,
		CURRENT_TIMESTAMP
		) RETURNING print_custom_order_id into id;
		output_msg:='Successfully inserted';	

		UPDATE TRACKER_ORDERS_PRINT SET customized='Y' WHERE order_id=$1;		
	
END
$BODY$
  LANGUAGE plpgsql;
