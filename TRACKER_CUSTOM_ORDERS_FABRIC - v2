CREATE TABLE TRACKER_CUSTOM_ORDERS_FABRIC
(
	fabric_custom_order_id serial NOT NULL UNIQUE,
	order_id int NOT NULL,
	fabric_length float NOT NULL,
	fabric_width float NOT NULL,
	fabric_shrinkage float,
	fabric_pcs int NOT NULL,
	fabric_total_measurement float NOT NULL,
	issued char(1) NOT NULL DEFAULT 'Y',
    issued_date timestamp,
    delivered char(1),
    actual_delivery_date timestamp,
	designer_received char(1),
	designer_received_date timestamp,
	PRIMARY KEY (fabric_custom_order_id)
) WITHOUT OIDS;
