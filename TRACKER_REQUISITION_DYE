CREATE TABLE TRACKER_REQUISITION_DYE
(
	dye_requisition_id serial NOT NULL,
	issue_min_date date NOT NULL,
	issue_max_date date NOT NULL,
	transaction_id varchar(100) NOT NULL,
	delivery_min_date date NOT NULL,
	delivery_max_date date NOT NULL,
	category_name text NOT NULL,
	style_code varchar(50) NOT NULL,
	designer_code varchar(20) NOT NULL,
	color_code varchar(10) NOT NULL,
	product_codes varchar(500) NOT NULL,
	item_code varchar(50) NOT NULL,
	production_types varchar(200) NOT NULL,
	dye_code varchar(50) NOT NULL,
	fabric_code varchar(50) NOT NULL,
	fabric_name varchar(100) NOT NULL,
	fabric_type varchar(20) NOT NULL,
	dye_length float NOT NULL,
	dye_width float NOT NULL,
	dye_pcs int NOT NULL,
	dye_total_measurement float NOT NULL,
	sample_reference varchar(20) NOT NULL,
	sc_planning_main_item_id int NOT NULL,
	style_ccode_id int NOT NULL,
	sc_asis_plan_dye_item_id int NOT NULL,
	sc_custom_plan_dye_item_id int NOT NULL,
	sc_custom_dept_plan_dye_item_id int NOT NULL,
	custom char(1),
	dept_changed char(1) NOT NULL,
	approved char(1) NOT NULL,
	PRIMARY KEY (dye_requisition_id)
) WITHOUT OIDS;
