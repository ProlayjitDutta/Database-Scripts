/* Create Foreign Keys */

ALTER TABLE TRACKER_CUSTOM_ORDERS_DYE
	ADD FOREIGN KEY (order_id)
	REFERENCES TRACKER_ORDERS_DYE (order_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;


ALTER TABLE TRACKER_CUSTOM_ORDERS_FABRIC
	ADD FOREIGN KEY (order_id)
	REFERENCES TRACKER_ORDERS_FABRIC (order_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;


ALTER TABLE TRACKER_CUSTOM_ORDERS_MATERIAL
	ADD FOREIGN KEY (order_id)
	REFERENCES TRACKER_ORDERS_MATERIAL (order_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;


ALTER TABLE TRACKER_CUSTOM_ORDERS_PRINT
	ADD FOREIGN KEY (order_id)
	REFERENCES TRACKER_ORDERS_PRINT (order_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;


ALTER TABLE TRACKER_ORDERS_DYE
	ADD FOREIGN KEY (dye_requisition_id)
	REFERENCES TRACKER_REQUISITION_DYE (dye_requisition_id)
	ON UPDATE RESTRICT
	ON DELETE RESTRICT
;


