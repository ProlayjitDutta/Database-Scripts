CREATE OR REPLACE FUNCTION func_tracker_save_custom_orders_fabric(
	IN v_order_id int,
	IN v_fabric_length float,
	IN v_fabric_width float,
	IN v_fabric_shrinkage float,
	IN v_fabric_pcs int,
	IN v_fabric_total_measurement float,
    OUT output_msg text,
    OUT id integer)
  RETURNS record AS
$BODY$
BEGIN   
					
		INSERT INTO tracker_custom_orders_fabric (
		  order_id,
		  fabric_length,
		  fabric_width,
		  fabric_shrinkage,
		  fabric_pcs,
		  fabric_total_measurement
		)
		VALUES(			
		$1,
		$2,
		$3,
		$4,
		$5,
		$6
		) RETURNING fabric_custom_order_id into id;
		output_msg:='Successfully inserted';		
	
END
$BODY$
  LANGUAGE plpgsql;
